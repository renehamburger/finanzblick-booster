{
  "manifest_version": 2,
  "name": "Finanzblick booster",
  "description": "Chrome extension to extend & fix the Finanzblick web app at https://finanzblick.de/webapp/",
  "version": "0.0.1",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://finanzblick.de/webapp/*"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "js/vendor.js",
      "js/background.js"
    ]
  },
  "permissions": [
    "storage"
  ]
}
